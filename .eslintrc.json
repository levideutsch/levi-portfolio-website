{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "react-app"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "no-unused-vars": [
        "warn", 
        {
          "varsIgnorePattern": "^_", 
          "argsIgnorePattern": "^_"
        }
      ]
    }
  }
  